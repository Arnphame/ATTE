{% extends 'base.html.twig' %}
{% block title %}Hello!{% endblock %}

{% block body %}
    <div class="container-fluid">
        <div class="text-left col-md-8 col-md-offset-2">
            {{ form_start(form) }}

        <br>
        <br>
        Registration number {{ form_widget(form.regNr) }}
        Make {{ form_widget(form.make) }}
        Model {{ form_widget(form.model) }}
        Year of production{{ form_widget(form.year) }}
        Fuel Type {{ form_widget(form.fuelType) }}
        Gearbox {{ form_widget(form.gearbox) }}
        Engine Capacity {{ form_widget(form.engineCapacity) }}

        <button type="submit" class="btn">Add</button>
        {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
